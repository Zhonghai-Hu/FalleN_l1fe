
 
long long dp[MAXV];
int a[MAXN];
int main()
{
    freopen("money.in","r",stdin);
    freopen("money.out","w",stdout);
    int N,V;
    scanf("%d %d",&N,&V);
    memset(dp,0,sizeof(dp));
    for(int i=0;i<N;i++)
        scanf("%d",&a[i]);
    dp[0]=1;
    for(int i=0;i<N;i++)
    {
        for(int j=a[i];j<=V;j++)
        {
            dp[j]+=dp[j-a[i]];
        }
    }
    printf("%lld\n",dp[V]);
    return 0;
}